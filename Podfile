# Uncomment the next line to define a global platform for your project
platform :osx, '10.15'

target 'MagicBar' do
    # Comment the next line if you don't want to use dynamic frameworks
    use_frameworks!

    # Pods for MagicBar
    pod 'RxSwift'
    pod 'RxCocoa'
    pod 'HotKey'

    target 'MagicBarTests' do
      inherit! :search_paths
      pod 'RxSwift'
      pod 'RxCocoa'
      pod 'RxBlocking'
      pod 'RxTest'
      pod 'Nimble'
    end
end
